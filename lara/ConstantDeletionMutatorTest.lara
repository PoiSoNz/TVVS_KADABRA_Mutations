import kadabra.mutation.ConstantDeletionMutator;
import weaver.WeaverJps;

/**
 * @test
 */
aspectdef ConstantDeletionMutatorTest

var $jp = WeaverJps.root();

var mutator = new ConstantDeletionMutator($jp, 'a');

while(mutator.hasMutations()) {
	// Print original
	print("original");
	println($jp.srcCode);
	
	// Mutate
	mutator.mutate();

	// Print mutation
	print("mutado");
	println($jp.srcCode);

	// Restore operator
	mutator.restore();

	// Print restore
	print("restaurado");
	println($jp.srcCode);
}

