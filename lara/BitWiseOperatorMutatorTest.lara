import kadabra.mutation.BitWiseOperatorMutator;
import weaver.WeaverJps;

aspectdef BitWiseOperatorMutatorTest

for($op of WeaverJps.search('expression').get()) {

	if($op.operator === '|') {
		var mutator = new BitWiseOperatorMutator($op, "rhs", "^", "lhs", "&"); // [">", "=="] is also supported

		while(mutator.hasMutations()) {
			// Mutate
			mutator.mutate();

			// Print
			println($op.ancestor('constructor').srcCode);

			// Restore operator
			mutator.restore();

		}
	}
}

end