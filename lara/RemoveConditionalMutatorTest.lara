import kadabra.mutation.RemoveConditionalMutator;
import weaver.WeaverJps;

aspectdef RemoveConditionalMutatorTest

var $jp = WeaverJps.root();

var mutator = new RemoveConditionalMutator($jp);

while(mutator.hasMutations()) {
	// Print original
	print("original");
	println($jp.srcCode);
	
	// Mutate
	mutator.mutate();

	// Print mutation
	print("mutado");
	println($jp.srcCode);

	// Restore operator
	mutator.restore();

	// Print restore
	print("restaurado");
	println($jp.srcCode);
}

end